{
  "name": "My Calculator",
  "description": "Calculator to do calculation",
  "version": "1.0",
  "manifest_version":2,
  "browser_action": {
    "default_icon": "images/img1.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "storage" //added storage permisson, so that can track changes in view.(Laxmann)
  ],
  "icons": {
    "16": "images/img1.png",
    "128": "images/img2.png"}
}
